{
    "name": "express-typescript-sequelize-boilerplate",
    "version": "1.0.0",
    "description": "Express-Typescript-Postgres-Sequelize Boilerplate",
    "main": "server.ts",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "start": "npm run build && node dist/server.js --production",
        "dev": "nodemon",
        "build": "tsc && tsc-alias && echo 'Build Finished! üëç'",
        "lint": "eslint --ignore-path .gitignore --ext .ts src/",
        "lint:fix": "npm run lint -- --fix",
        "migration:generate": "sequelize migration:generate --name",
        "migration": "sequelize db:migrate",
        "seed": "sequelize db:seed:all",
        "migration:undo": "sequelize db:migrate:undo",
        "migration:undo:all": "sequelize db:migrate:undo:all"
    },
